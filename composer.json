{
  "name": "spresnac/logcrawler-client",
  "description": "Client for logcrawler.de",
  "keywords": [
    "php",
    "laravel",
    "logs",
    "logging"
  ],
  "type": "library",
  "require": {
    "php": "^7.1|^7.2|^7.3|^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "monolog/monolog": "^1.12|^2",
    "illuminate/support": "^5.6|^6|^7|^8"
  },
  "require-dev": {
    "phpunit/phpunit": "^8|^9",
    "symfony/var-dumper": "^3|^4"
  },
  "autoload": {
    "psr-4": {
      "spresnac\\": "src/spresnac/"
    }
  },
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Sascha Presnac",
      "email": "sascha.presnac@protonmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "spresnac\\logcrawlerclient\\Providers\\LogcrawlerClientServiceProvider"
      ]
    }
  },
  "homepage": "https://logcrawler.de",
  "support": {
    "issues": "https://github.com/spresnac/logcrawler-client/issues",
    "source": "https://github.com/spresnac/logcrawler-client"
  },
  "scripts": {
    "test-ci": "vendor/bin/phpunit --coverage-clover coverage.xml"
  },
  "funding": [
    {
        "type": "other",
        "url": "https://www.buymeacoffee.com/spresnac"
    },
    {
      "type": "paypal",
      "url": "https://www.paypal.me/paladin"
    },
    {
        "type": "github",
        "url": "https://github.com/sponsors/spresnac"
    }
  ]
}
